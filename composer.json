{
    "name": "lampager/lampager-laravel",
    "description": "Rapid pagination for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "keywords": ["laravel", "illuminate", "pagination", "paginator", "offset", "limit"],
    "autoload": {
        "psr-4": {
            "Lampager\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lampager\\Laravel\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "lampager/lampager": "^0.4",
        "illuminate/contracts": "^9.0 || ^10.0 || ^11.0",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "illuminate/database": "^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "orchestra/testbench-core": ">=7.0",
        "phpunit/phpunit": ">=9.5",
        "nilportugues/sql-query-formatter": "^1.2.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Lampager\\Laravel\\MacroServiceProvider"
            ]
        }
    }
}
